{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "启动开发服务器",
			"type": "shell",
			"command": "pnpm",
			"args": [
				"run",
				"start:dev"
			],
			"group": "build",
			"isBackground": true,
			"problemMatcher": [
				"$tsc"
			],
			"options": {
				"env": {
					"NODE_ENV": "development"
				}
			}
		}
	]
}